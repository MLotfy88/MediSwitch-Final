    "Adult_Dosing": {
      "text": "",
      "subsections": {
        "RSI": {
          "text": "0.6 to 1.2mg/kg IV (ideal body weight)\nOnset of action faster with higher dose\nOne study suggests 2.0mg/kg may lead to optimal intubating conditions; however higher doses lead to longer duration of paralysis\n[1]",
          "tables": []
        },
        "Maintenance_Bolus_Dosing": {
          "text": "0.1 to 0.2mg/kg IV",
          "tables": []
        },
        "Infusion": {
          "text": "10 to 12 mcg/kg/min",
          "tables": []
        }
      },
