    },
    "Background": {
      "text": "Stridor refers to harsh upper airway sounds, classically inspiratory",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Clinical_Features": {
      "text": "Inspiratory stridor\nSuggestive of extrathoracic obstruction (Pressure\ntrach\n< Pressure\natm\n)\nforeign body\n,\nepiglottitis\nExpiratory stridor vs.\nwheezing\nSuggestive of intrathoracic obstruction (Pressure\ntrach\n< Pressure\npleura\n)\nAsthma",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "foreign body",
          "url": "https://wikem.org/wiki/Aspirated_foreign_body"
        },
        {
          "text": "epiglottitis",
          "url": "https://wikem.org/wiki/Epiglottitis"
        },
        {
          "text": "wheezing",
          "url": "https://wikem.org/wiki/Wheezing"
        },
        {
          "text": "Asthma",
          "url": "https://wikem.org/wiki/Asthma"
        }
      ]
    },
    "Differential_Diagnosis": {
      "text": "",
      "subsections": {
        "Trauma": {
          "text": "Larynx fracture\nTracheobronchial tear/injury\nThyroid gland injury/trauma\nTracheal injury\nElectromagnetic or\nradiation exposure\nBurns\n,\ninhalation injury",
          "tables": []
        },
        "Infectious_Disorders": {
          "text": "Bacterial tracheitis\nDiphtheria\nTetanus\nTracheobronchial\ntuberculosis\nPoliomyelitis\n, paralytic, bulbar, or acute\nFungal\nlaryngitis",
          "tables": []
        },
        "Abscesses": {
          "text": "Retropharyngeal abscess\nEpiglottitis\n, acute\nPeritonsillar abscess\nLaryngotracheobronchitis (\ncroup\n)\nRetropharyngeal abscess",
          "tables": []
        },
        "Neoplastic_Disorders": {
          "text": "Neoplasms/tumors",
          "tables": []
        },
        "Allergic_and_Auto-Immune_Disorders": {
          "text": "Spasmodic/tracheobronchitis\nAngioedema\n/Angioneurotic edema",
          "tables": []
        },
        "Metabolic,_Storage_Disorders": {
          "text": "Cerebral Gaucher's of infants (acute)\nTracheobronchial\namyloidosis",
          "tables": []
        },
        "Biochemical_Disorders": {
          "text": "Tetany",
          "tables": []
        },
        "Congenital,_Developmental_Disorders": {
          "text": "Angioedema\n/Angioneurotic edema, hereditary",
          "tables": []
        },
        "Psychiatric_Disorders": {
          "text": "Somatization disorder",
          "tables": []
        },
        "Anatomical_or_Mechanical": {
          "text": "Foreign body aspiration\nAcute gastric acid/aspiration syndrome\nAirway obstruction\nNeck compartment hemorrhage/hematoma\nParadoxical vocal fold motion\n[1]",
          "tables": []
        },
        "Vegetative,_Autonomic,_Endocrine_Disorders": {
          "text": "Esophageal free reflux/\nGERD\nsyndrome\nLaryngospasm, acute\nBilateral vocal cord paralysis\nHypoparathyroidism",
          "tables": []
        },
        "Poisoning": {
          "text": "Smoke inhalation\nChemical burn/esophagus",
          "tables": []
        },
