    "Management": {
      "text": "",
      "subsections": {
        "Supportive": {
          "text": "Hypotension\nIVF\nNorepinephrine\nQT prolongation\nTreat all with QTc >500ms with\nmagnesium\n2-4gm IV over 10min",
          "tables": []
        },
        "Extrapyramidal": {
          "text": "Diphenhydramine\n25-50mg IV/IM\nOR\nbenztropine\n1-2mg IV/IM\nOral therapy with either of above meds should be continued for 2 weeks",
          "tables": []
        }
      },
