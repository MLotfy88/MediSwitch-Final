  "sections": {
    "Background": {
      "text": "Placenta that extends near, partially over, or beyond the internal cervical os\nCan be total, partial, or marginal\nDo NOT perform digital or speculum exam",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Risk_Factors": {
      "text": "Uterine scar\nAdvanced maternal age\nCigarette smoking\nCocaine\nabuse",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Cocaine",
          "url": "https://wikem.org/wiki/Cocaine"
        }
      ]
    },
    "Clinical_Features": {
      "text": "Painless\nvaginal bleeding\nin 3rd trimester\nMay be profuse, bright red",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "vaginal bleeding",
          "url": "https://wikem.org/wiki/Vaginal_bleeding_in_pregnancy_(greater_than_20wks)"
        }
      ]
    },
