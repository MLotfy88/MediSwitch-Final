  "sections": {
    "General": {
      "text": "Type:\nBeta-2 agonist\nDosage Forms: aerosol inhaler, liquid nebulized form, PO\nCommon Trade Names: ProAir HFA, Proventil, Ventolin HFA, ReliOn Ventolin HFA, Accuneb",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Beta-2 agonist",
          "url": "https://wikem.org/wiki/Beta-2_agonist"
        }
      ]
    },
    "Adult_Dosing": {
      "text": "Mild Asthma/COPD - 90mcg/inh via inhalation aerosol q4hrs PRN wheezing\nMod/Severe Asthma/COPD exacerbation - 5mg Nebulized q15mins\nHyperkalemia\n(not-FDA approved) - 15-20mg Nebulized",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Hyperkalemia",
          "url": "https://wikem.org/wiki/Hyperkalemia"
        }
      ]
    },
    "Pediatric_Dosing": {
      "text": "Mild asthma exacerbation - 1-2 puffs of 90mcg/inh via inhaled aerosol q4hrs PRN wheezing\nMod/Severe asthma exacerbation - 2.5mg nebulized q20mins\nWheezing - 2.5mg nebulized q20mins",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Special_Populations": {
      "text": "Pregnancy Rating\n: C\nLactation Risk Categories\n: Probably safe\nRenal Dosing\nAdult: no adjustment\nPediatric: no adjustment\nHepatic Dosing\nAdult: not defined\nPediatric: not defined",
      "subsections": {},
