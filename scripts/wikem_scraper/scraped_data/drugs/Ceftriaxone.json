  "scraped_at": "2026-01-10T07:50:05.108381",
  "sections": {
    "General": {
      "text": "Type: 3rd generation\nCephalosporin\nDosage Forms: injectable solution, powder for injection\nDosage Strengths: injectable solution: 1g/50mL, 2g/50mL; powder for injection: 250mg, 500mg, 1g, 2g, 10g, 100g\nRoutes of Administration: IV, IM\nCommon Trade Names: Rocephin",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Cephalosporin",
          "url": "https://wikem.org/wiki/Cephalosporin"
        }
      ]
    },
    "Adult_Dosing": {
      "text": "",
      "subsections": {
        "General_2": {
          "text": "1-2g IM/IV q24h\nFirst ED Dose: 1g IM/IV x 1\nMax: 4g/24h",
          "tables": []
        },
        "Bacterial_Meningitis": {
          "text": "4g IV divided q12-24h x 7-21 days\nFirst ED Dose: 2g IM/IV x 1",
          "tables": []
        },
        "Gonococcal": {
          "text": "Uncomplicated, initial: 250mg IM x 1\nAlso treat for chlamydia regardless of test results\nUncomplicated, recurrent of persistent: 500-1000mg IM x 1\nDisseminated: 1g IM/IV q24h\nMay switch to cefixime PO after 24-48h if clinical improvement\nConjunctivitis: 1g IM x 1\nProphylaxis (sexual assault victims)\n250mg IM x 1",
          "tables": []
        },
        "PID": {
          "text": "250mg IM x 1\nUse with doxycycline +/- metronidazole",
          "tables": []
        },
        "Sinusitis,_Severe": {
          "text": "1-2g IV q12-24h x 7-10 days",
          "tables": []
        },
        "Endocarditis": {
          "text": "1g IM/IV x 1\n30-60 min before procedure",
          "tables": []
        },
        "Proctitis,_Epididymitis,_Chancroid": {
          "text": "250mg IM x 1\nGive with doxycycline",
          "tables": []
        },
        "Surgical_Prophylaxis": {
          "text": "1g IV x 1\nGive 0.5-2h preop",
          "tables": []
        },
        "Typhoid_Fever": {
          "text": "60mg/kg IV q24h x 2wk\nMax: 4g/24h",
          "tables": []
        }
      },
      "tables": [],
      "links": []
    },
