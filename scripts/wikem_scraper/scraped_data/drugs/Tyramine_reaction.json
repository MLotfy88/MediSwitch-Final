    "Evaluation": {
      "text": "ECG\n: monitor for cardiac ischemia\nCT head: if patient has focal neurologic findings or persistent, severe headache",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "ECG",
          "url": "https://wikem.org/wiki/ECG"
        }
      ]
    },
    "Management": {
      "text": "Hypertensive emergency\n: phentolamine is agent of choice\n2.5-5mg IV q5-15min until blood pressure controlled\nDuration of action <1 hour\nBeta-blockers\ncontraindicated",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Hypertensive emergency",
          "url": "https://wikem.org/wiki/Hypertensive_emergency"
        }
      ]
    },
    "Disposition": {
      "text": "Discharge home if asymptomatic after 4 hours of observation",
      "subsections": {},
      "tables": [],
      "links": []
