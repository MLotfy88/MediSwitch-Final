    "Management": {
      "text": "",
      "subsections": {
        "Synchronized_Cardioversion": {
          "text": "Adults: 0.5-1.0 J/kg\nPediatrics: 0.5-1 J/kg\nIf unsuccessful, increase to 2 J/kg\nConsider sedation prior to cardioversion only if it will not significantly delay the procedure",
          "tables": []
        },
        "Verapamil": {
          "text": "10 mg over 1 min\nOnly administer in stable patients with an established diagnosis of IFLVT",
          "tables": []
