{
  "drug_name": "Ring_avulsion_injury",
  "url": "https://wikem.org/wiki/Ring_avulsion_injury",
  "scraped_at": "2026-01-10T08:01:35.283927",
  "sections": {
    "Background": {
      "text": "Sudden pull on a ring results in severe soft tissue avulsion injury\nCan range from circumferential soft tissue laceration to complete amputation\nOften with concomitant neurovascular damage",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Clinical_Features": {
      "text": "Typically only one digit involved\nHistory of working with machinery or getting digit caught in door\nPain, bleeding, lack of sensation",
      "subsections": {
        "Classes_of_Injury": {
          "text": "Based on circulatory status\nClass I: Circulation adequate\nClass II: Circulation inadequate\nClass III: Complete degloving or complete amputation",
          "tables": []
        }
      },
      "tables": [],
      "links": []
    },
