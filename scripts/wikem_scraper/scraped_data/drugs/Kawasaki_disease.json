    "Evaluation": {
      "text": "",
      "subsections": {
        "Work-Up": {
          "text": "CBC\nLeukocytosis\nThrombocytosis\nLabs\nLFTs\n- elevated\nESR, CRP- elevated\nBlood culture\nUrinalysis\nECG\nViral panel: A positive viral PCR does not exclude KD\n42% of children with KD had a +viral respiratory panel on initial presentation\nTTE\n(Coronaries, LV, valves)\nConsider if f ESR≥ 40, CRP≥ 3, or if desquamation occurs\nRed Top \"Kawasaki Serum to CBR\"",
          "tables": []
        },
        "Evaluation_2": {
          "text": "Clinical diagnosis",
          "tables": [
            [
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "1. Bilateral nonexudativeconjunctivitis"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "2. Mucous membrane changes (erythema, peeling, cracking of lips, \"strawberry tongue,\" or diffuse oropharyngeal mucosal erythema)"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "3. Changes of the extremities (erythema or swelling of hands/feet, peeling of finger tips/toes in the convalescent stage)"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "4.Rash"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "5. Cervical adenopathy (more than one node >1.5 cm unusually unilateral anterior cervical)"
              },
              {
                "Classic Kawasaki Disease": "Incomplete Kawasaki Disease",
                "Fever for 5 days or moreplusfour of the following symptoms": "Fever for 5 daysandtwo to three clinical criteria of classic Kawasaki diseaseplus"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "C-reactive protein 3.0 milligrams/L and/or erythrocyte sedimentation rate 40 mm/hpluspositive echocardiogramorthree or more of the following:"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "1. Albumin <3 grams/dL"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "2.Anemia"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "3. Elevated alanine aminotransferase (ALT)"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "4. Platelets >450,000/mm3 7d after fever onset"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "5. White blood cell count >12,000/mm3"
              },
              {
                "Classic Kawasaki Disease": "",
                "Fever for 5 days or moreplusfour of the following symptoms": "6. Pyuria"
              }
            ]
          ]
        }
      },
