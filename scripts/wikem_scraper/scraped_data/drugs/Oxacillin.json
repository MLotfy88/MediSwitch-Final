    "Pediatric_Dosing": {
      "text": "",
      "subsections": {
        "General_3": {
          "text": "Mild-to-moderate\n50mg/kg/day IM/IV divided q6 hours\nFirst Dose: 12.5mg/kg IM/IV x 1\nMax: 4 g/day\nSevere\n100mg/kg/day IM/IV divided q4-6 hours\nFirst Dose: 16.6-25mg/kg IM/IV x 1\nMax: 12 g/day",
          "tables": []
        },
        "Community-Acquired_Pneumonia_(>3_Months)": {
          "text": "150-200mg/kg/day divided q6-8 hours\nFirst Dose: 37.5-66.6mg/kg IV x 1\nFor\nMSSA",
          "tables": []
        }
      },
      "tables": [],
      "links": [
