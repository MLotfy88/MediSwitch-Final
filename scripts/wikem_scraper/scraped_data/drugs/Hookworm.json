    "Evaluation": {
      "text": "Stool ova and parasites - ova found in stool\nStool culture\nCBC\nHypochromic, microcytic anemia\nEosinophilia\n, often marked",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Eosinophilia",
          "url": "https://wikem.org/wiki/Eosinophilia"
        }
      ]
    },
    "Management": {
      "text": "Albendazole 400mg x 1 dose (high efficacy)\nOR\nmebendazole 500mg x 1 dose (low to moderate efficacy)\nIron supplementation for\nanemia",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "anemia",
          "url": "https://wikem.org/wiki/Anemia"
        }
      ]
    },
    "Disposition": {
      "text": "Generally may be discharged",
