    "Adult_Dosing": {
      "text": "",
      "subsections": {
        "General_2": {
          "text": "Mild-to-moderate infection\n250-500mg IM/IV q4-6 hours\nSevere infection\n1g IM/IV q4-6 hours",
          "tables": []
        },
        "Endocarditis": {
          "text": "2g IV q4 hours +\ngentamicin",
          "tables": []
        },
        "Prosthetic_Joint_Infection": {
          "text": "2g IV q4 hours +\nrifampin",
          "tables": []
        },
