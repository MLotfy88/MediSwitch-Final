              {
                "Synovium": "WBC",
                "Normal": "<200",
                "Noninflammatory": "<200-2000",
                "Inflammatory": "200-50,000",
                "Septic": ">1,100 (prosthetic joint)>25,000; LR=2.9>50,000; LR=7.7>100,000; LR=28"
              },
              {
                "Synovium": "PMN",
                "Normal": "<25%",
