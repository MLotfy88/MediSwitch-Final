    "Management": {
      "text": "Full\nsepsis\nwork-up\nIV\nantibiotics\nand\nfluids\nPressors\nif CHD suspected\nIntubate\nand give\nPGE\nSides effects of\nPGE\ninclude apnea (10%)\nIf\ninborn error of metabolism\nsuspected\nIV\ndextrose\nat 1.5 maintenance\nDialysis\nif ammonia >200",
      "subsections": {
        "Empiric_Treatment_for_Unstable_Neonates": {
          "text": "",
          "tables": [
            [
              {
                "Medication/Intervention": "Glucose",
                "Indication": "Hypoglycemia",
                "Dose/Size (for neonate)": "5–10 mL/kg of10% dextrose in waterIV"
              },
              {
                "Medication/Intervention": "3% normal saline",
                "Indication": "Symptomatichyponatremia",
                "Dose/Size (for neonate)": "3–5 mL/kg bolus IV"
              },
              {
                "Medication/Intervention": "Calcium",
                "Indication": "Hypocalcemia",
                "Dose/Size (for neonate)": "50–100 milligrams/kgcalcium gluconateor 20 milligrams/kgcalcium chlorideIV"
              },
              {
                "Medication/Intervention": "Cefotaxime",
                "Indication": "Infection",
                "Dose/Size (for neonate)": "50 milligrams/kg IV"
              },
              {
                "Medication/Intervention": "Ampicillin",
                "Indication": "Infection",
                "Dose/Size (for neonate)": "50 milligrams/kg IV"
              },
              {
                "Medication/Intervention": "Gentamicin",
                "Indication": "Infection",
                "Dose/Size (for neonate)": "2.5 milligrams/kg IV"
              },
              {
                "Medication/Intervention": "Packed red blood cells",
                "Indication": "Anemia",
                "Dose/Size (for neonate)": "10 mL/kg IV"
              },
              {
                "Medication/Intervention": "Normal saline",
                "Indication": "Hypotension,dehydration",
                "Dose/Size (for neonate)": "5-10 mL/kg IV aliquots (up to 60-80 mL/kg)"
              },
              {
                "Medication/Intervention": "10%dextrosein one fourth normal saline",
                "Indication": "Metabolic disease",
                "Dose/Size (for neonate)": "1.5 maintenance (6 mL/kg/h for the first 10 kg)"
              },
              {
                "Medication/Intervention": "Endotrachealintubation",
                "Indication": "Hypoventilation or frequent apnea",
                "Dose/Size (for neonate)": "<3mm for preemie; 3mm for term neonate, 3.5mm for older infant; cuffed tube prefered if not premature"
              }
            ]
          ]
        }
      },
