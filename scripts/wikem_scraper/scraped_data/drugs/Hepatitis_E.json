    "Evaluation": {
      "text": "",
      "subsections": {
        "Workup": {
          "text": "LFTs\nINR\nCoagulopathy\ncorrelates w/more severe liver dysfunction\nAcute hepatitis panel\nHep A Ab IgM\nHep B cAb IgM\nHep B sAg\nHep B sAb\nHep C Ab",
          "tables": []
        },
        "Diagnosis": {
          "text": "Clinically indistinguishable from other viral hepatitides, no serologic test available\n[4]",
          "tables": []
        },
        "Interpreting_Acute_Hepatitis_Panel_Results": {
          "text": "",
          "tables": [
            [
              {
                "Anti-hepatitis A, IgM": "Positive",
