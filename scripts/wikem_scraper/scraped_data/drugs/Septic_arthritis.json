    "Management": {
      "text": "",
      "subsections": {
        "Antibiotics": {
          "text": "For adults treatment should be divided into Gonococcal and Non-Gonococcal",
          "tables": []
        },
        "Gonococcal": {
          "text": "Ceftriaxone\n1g IV once daily\nCefixime\n400 mg PO BID is an option for outpatient therapy after initial 3 days of\nCeftriaxone",
          "tables": []
        },
        "Non-Gonococcal": {
          "text": "Treatment should cover\nS. aureus\n,\nStreptococcus\n,\nPseudomonas\n,\nEnterococcus\n,\nB. burgdorferi\nVancomycin 15-20 mg/kg IV BID PLUS any of the following:\nCeftriaxone\n2g IV once daily\nCefepime\n2g IV three times daily\nCeftazidime\n2g IV three times daily\nCiprofloxacin\n400mg IV three times daily",
          "tables": []
        },
        "Pediatrics": {
          "text": "Ceftriaxone\n1g IV once daily",
          "tables": []
        },
        "Sickle_Cell": {
          "text": "Coverage for\nSalmonella\nand\nStaphylococcus\nspp\nVancomycin\n20mg/kg IV twice daily PLUS\nCiprofloxacin 400mg IV three times daily OR\nImipenem/cilastatin 1g IV three times daily",
          "tables": []
        },
        "Consultation": {
          "text": "Consult ortho for joint irrigation in OR if joint aspirate is indicative of infection\nBenefit of serial aspirations vs arthroscopy vs irrigation and debridement is unclear",
          "tables": []
        }
      },
      "tables": [],
