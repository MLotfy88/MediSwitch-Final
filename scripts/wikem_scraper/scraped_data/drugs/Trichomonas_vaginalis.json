    "Management": {
      "text": "",
      "subsections": {
        "Non-Pregnant": {
          "text": "Metronidazole\n500mg PO BID for 7 days\n[2]\nTinidazole\n2g PO once",
          "tables": []
        },
        "Pregnant": {
          "text": "Only treat if the patient is symptomatic\nMetronidazole\n500mg PO BID for 7 days\n[3]",
          "tables": []
        },
        "Sexual_Partner_Treatment": {
          "text": "Female: Same as above\nMale:\nMetronidazole\n2 gm PO x1\n[4]",
          "tables": []
        }
