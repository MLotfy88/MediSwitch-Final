    "Evaluation": {
      "text": "",
      "subsections": {
        "Work-Up": {
          "text": "ECG\n+/- telemetry\nTroponin\nif possible acute event",
          "tables": []
        },
        "Diagnosis": {
          "text": "Rule out\nunstable angina\n:\nNew angina\nAngina at rest\nAccelerating frequency or severity",
          "tables": []
        },
        "History": {
          "text": "Assess:\nChanges in angina or heart failure symptoms\nAdherence to prescribed medications\nChanges in medications\nMedication side effects",
          "tables": []
        },
        "Physical_Examination": {
          "text": "Resting heart rate and blood pressure\nSigns of heart failure\nNew dysrhythmia\nNew or worsening vascular bruits or murmurs\nStatus of abdominal aorta",
          "tables": []
        }
      },
