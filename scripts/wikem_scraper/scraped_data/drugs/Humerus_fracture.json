    "Management_&_Disposition": {
      "text": "",
      "subsections": {
        "General_Fracture_Management": {
          "text": "Acute pain management\nOpen fractures\nrequire immediate IV antibiotics and urgent surgical washout\nNeurovascular compromise from fracture requires emergent reduction and/or orthopedic intervention\nConsider risk for\ncompartment syndrome",
          "tables": []
        },
        "Adult_Humerus_Fracture_Management_Table": {
          "text": "",
          "tables": [
            [
              {
                "Fracture": "Proximal",
                "Splint": "Sling and swathe splint, orcoaptation splint",
                "Disposition": "Non-emergent, but many need surgery, refer to ortho vs ED consult"
              },
              {
                "Fracture": "Shaft",
                "Splint": "U-shapedcoaptation splint(upper arm sugar-tong splint) +sling and swathe",
                "Disposition": "R/o neurovasc injury and compartment syndrome, but many need surgery, refer to ortho vs ED consult"
              },
              {
                "Fracture": "Elbow Fracture (Adult)",
                "Splint": "Long arm posterior splint",
                "Disposition": "R/o neurovasc injury and compartment syndrome, but many need surgery, refer to ortho vs ED consult"
              },
              {
                "Fracture": "Olecranon",
                "Splint": "Nondisplaced:Sling and Swathe SplintDisplaced:Long Arm Posterior Splintor emerg. ORIF",
                "Disposition": "R/o neurovasc injury and compartment syndrome, refer to ortho within 24 hrs"
              }
            ]
          ]
        }
      },
      "tables": [],
