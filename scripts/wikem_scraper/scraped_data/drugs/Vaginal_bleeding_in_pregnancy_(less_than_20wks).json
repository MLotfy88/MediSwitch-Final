  "sections": {
    "Background": {
      "text": "Occurs in 20-40% of 1st trimester pregnancies\nOnce IUP is confirmed by\nultrasound\nno utility in obtaining\nB-hCG\nUS\nDo not use hCG to determine whether\nultrasound\nshould be obtained",
      "subsections": {
        "Abortion_Types": {
          "text": "",
          "tables": [
            [
              {
                "Classification": "Threatened",
                "Characteristics": "Abdominal pain or bleeding; < 20 weeks gestation",
                "OS": "Closed",
                "Fetal Tissue Passage": "No",
                "Misc": "If  < 11 weeks (with fetal cardiac activity) 90% progress to term.  If between 11 and 20 weeks 50% progress to term"
              },
              {
                "Classification": "Inevitable",
                "Characteristics": "Abdominal pain or bleeding; < 20 weeks gestation",
                "OS": "Open",
                "Fetal Tissue Passage": "No",
                "Misc": ""
              },
              {
                "Classification": "Incomplete",
                "Characteristics": "Abdominal pain or bleeding; < 20 weeks gestation",
                "OS": "Open",
                "Fetal Tissue Passage": "Yes, some",
                "Misc": ""
              },
              {
                "Classification": "Complete",
                "Characteristics": "Abdominal pain or bleeding; < 20 weeks gestation",
                "OS": "Closed",
                "Fetal Tissue Passage": "Yes, complete expulsion of products",
                "Misc": "Distinguish from ectopic based on decreasing hCG and/or decreased bleeding"
              },
              {
                "Classification": "Missed",
                "Characteristics": "Fetal death at <20 weeks without passage of any fetal tissue for 4 weeks after fetal death",
                "OS": "Closed",
                "Fetal Tissue Passage": "No",
                "Misc": ""
              },
              {
                "Classification": "Septic",
                "Characteristics": "Infection of the uterus during a miscarriage.  Most commonly caused by retained products of conception",
                "OS": "Open",
                "Fetal Tissue Passage": "No, or may be incomplete",
                "Misc": "Uterine tenderness and purulent discharge from the OS may be present"
              }
            ]
          ]
        }
