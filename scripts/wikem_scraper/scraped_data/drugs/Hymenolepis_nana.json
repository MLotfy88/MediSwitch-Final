    "Evaluation": {
      "text": "History of above symptoms +/- local exposure\nPeripheral blood eosinophilia of 5 to 10% may be observed\nDefinitive diagnosis by identifying eggs or proglottids in the stool\nDiagnosis of hymenolepiasis should prompt family screening or empiric treatment",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Management": {
      "text": "Praziquantel\n: 25mg/kg orally (single dose), followed by repeat dose 10 days later",
      "subsections": {},
      "tables": [],
      "links": [
        {
          "text": "Praziquantel",
          "url": "https://wikem.org/wiki/Praziquantel"
        }
      ]
    },
    "Disposition": {
      "text": "Discharge with follow up and plan to either screen or treat family and close contacts",
      "subsections": {},
      "tables": [],
      "links": []
    }
  }
