    "Pediatric_Dosing": {
      "text": "",
      "subsections": {
        "General_(<7_Days_Old)": {
          "text": "50mg/kg IM/IV q24h\nFirst Dose: 50mg/kg IM/IV x 1",
          "tables": []
        },
        "General_(7_Days_-_1_Month)": {
          "text": "<2000g\n50mg/kg IM/IV q24h\nFirst Dose: 50mg/kg IM/IV x 1\n>2000g\n50-75mg/kg IM/IV q24h\nFirst Dose: 50-75mg/kg IM/IV x 1",
          "tables": []
        },
        "General_(>1_Month_-_Adult)": {
          "text": "50-100mg/kg IM/IV divided q12-24h\nFirst Dose: 50-100mg/kg IM/IV x 1\nMax: 4g/24h",
          "tables": []
        },
        "Acute_Otitis_Media_(2_Months_-_12_Years)": {
          "text": "50mg/kg IM/IV x 1\nGive 50mg/kg IM/IV q24h x 3 days for recurrent infection\nMax 1g/dose",
          "tables": []
        },
        "Bacterial_Meningitis_2": {
          "text": "80-100mg/kg IV divided q12-24h x 7-21 days\nFirst Dose: 100mg/kg IV x 1\nMax: 4g/24h",
          "tables": []
        },
        "Pneumonia,_Community-Acquired_(>3_Months)": {
          "text": "50-100mg/kg/day IM/IV divided q12-24h x 10 days\nFirst Dose:\nMax: 2g/day\nInfo: May switch to PO when possible to complete course",
          "tables": []
        }
      },
