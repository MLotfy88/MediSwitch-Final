  "sections": {
    "Background": {
      "text": "Airway adjunct used to open oropharynx and increase effectiveness of BVM ventilation",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Indications": {
      "text": "Unresponsive patients without gag reflex\nApneic patient being ventilated with BVM",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Contraindications": {
      "text": "Conscious patients\nIntact gag reflex",
      "subsections": {},
      "tables": [],
      "links": []
    },
    "Procedure": {
      "text": "",
      "subsections": {
        "Application": {
          "text": "Size\nOPA\nfrom earlobe to mouth\nOpen patients mouth and insert\nOPA\nwith the tip towards the roof of the mouth\nRotate airway 180° and insert until it is fully inserted",
          "tables": []
        },
        "Removal": {
          "text": "Pull out the OPA",
          "tables": []
        }
