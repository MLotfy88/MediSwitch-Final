    "Clinical_Features": {
      "text": "Shoulder pain\nDecreased shoulder range of motion",
      "subsections": {
        "Comparison_of_Shoulder_Dislocation_Clinical_Features": {
          "text": "",
          "tables": [
            [
              {
                "Finding": "Arm position",
                "Anterior(~95%)": "Arm maintained in abduction and external rotation",
                "Posterior(~5%)": "Posterior aspect of shoulder unusually prominent",
                "Inferior(<1%)": "Humerus fully abducted / Hand on or behind head"
              },
              {
                "Finding": "Shoulder appearance",
                "Anterior(~95%)": "Loss of normal rounded appearance with stretching of the deltoid muscle (i.e., \"squared off\")",
                "Posterior(~5%)": "Anterior aspect of shoulder appears flattened",
                "Inferior(<1%)": ""
              },
              {
                "Finding": "Range of motion",
                "Anterior(~95%)": "Difficulty touching affected arm to contralateral shoulder due to pain",
                "Posterior(~5%)": "Inability to rotate or abduct affected arm",
                "Inferior(<1%)": "Humeral head palpable in axilla or lateral chest wall"
              },
              {
                "Finding": "Mechanism",
                "Anterior(~95%)": "Most common; range of mechanisms",
                "Posterior(~5%)": "Forceful internal rotation and adduction (e.g., blow to anterior shoulder, seizure, eletric shock)",
                "Inferior(<1%)": "Forceful hyper-abduction of arm"
              }
            ]
          ]
        }
      },
