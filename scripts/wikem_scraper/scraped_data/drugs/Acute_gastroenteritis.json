    "Background": {
      "text": "Blood diarrhea suggests bacterial etiology\nViral AGE usually lasts <7d\nDo not diagnose isolated vomiting as AGE",
      "subsections": {
        "Causes": {
          "text": "",
          "tables": [
            [
              {
                "Species": "Viral(norovirus,adenovirus,rotavirus)",
                "Onset": "11-72 hrs",
                "Symptoms": "Nausea, vomiting, watery diarrheaMild abdominal cramps, myalgia",
                "Transmission": "Fecal-oralContaminated food or water",
                "Preformed Toxin": "No"
              },
              {
                "Species": "Staph",
                "Onset": "1-6 hrs",
                "Symptoms": "Nausea, severe vomiting, diarrhea,Mild abdominal cramping",
                "Transmission": "Previously cooked foods (mayonaise, ham, salads)",
                "Preformed Toxin": "Yes"
              },
              {
                "Species": "B. cereus",
                "Onset": "1-6 hrs",
                "Symptoms": "Abrupt onset of nausea, vomiting, mild diarrhea",
                "Transmission": "Previously cooked foods (rice, vegetables, dried fruits, meat)",
                "Preformed Toxin": "Yes"
              },
              {
                "Species": "C. perfringens",
                "Onset": "8-24 hrs",
                "Symptoms": "Nausea, minimal vomiting, watery diarrheaAbd cramps",
                "Transmission": "Previously cooked or reheated meats and poultry",
                "Preformed Toxin": "Yes"
              },
              {
                "Species": "V. cholerae",
                "Onset": "11-72 hrs",
                "Symptoms": "Explosive rice-water diarrheaVomiting, abdominal crampsFever",
                "Transmission": "Fecal-oralContaminated food or water",
                "Preformed Toxin": "Yes"
              },
              {
                "Species": "Giardia",
                "Onset": "1-4 wks",
                "Symptoms": "Flatus, bloatingFoul-smelling and fatty stools (steatorrhea)",
                "Transmission": "Fecal-oralContaminated water",
                "Preformed Toxin": "No"
              }
            ],
            [
              {
                "Species": "Salmonella",
                "Onset": "6-72 hours",
                "Symptoms": "Fever, relative bradycardiaAbdominal painBloody diarrheaHeadacheOsteomyelitis in sickle cell",
                "Transmission": "EggsPoultryWaterReptiles"
              },
              {
                "Species": "Shigella",
                "Onset": "1-3 days",
                "Symptoms": "FeverAbdominal painBloody diarrheaHeadache1-5 years oldSeizures in children",
                "Transmission": "FoodFecal-Oral"
              },
              {
                "Species": "Yersinia",
                "Onset": "1-5 days",
                "Symptoms": "Appendicitismimic,RLQ painFeverVomitingKids and Young Adults",
                "Transmission": "WaterMilkPorkWild AnimalsFecal-Oral"
              },
              {
                "Species": "Campylobacter",
                "Onset": "1-7 days",
                "Symptoms": "Low gradefeverAbdominal painKids and Young AdultsGuillain-Barre",
                "Transmission": "WaterPoultryPets/Animals"
              },
              {
                "Species": "C. Diff",
                "Onset": "1-11 Weeks",
                "Symptoms": "Copious FOUL diarrhea",
                "Transmission": "Antibiotic use:PCN,Clinda,Cephalosporins"
              },
              {
                "Species": "Entamoeba",
                "Onset": "1-11 weeks",
                "Symptoms": "AppendicitisMimicAbdominal pain,nausea/vomiting,diarrhea",
                "Transmission": "WaterSanitationTravel"
              }
            ]
